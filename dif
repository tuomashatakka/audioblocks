+        this.emit('presenceSync', state);
+        this.lastPingTime = Date.now();
+      })
+      // Listen to users joining
+      .on('presence', { event: 'join' }, ({ key, newPresences }) => {
+        this.emit('presenceJoin', { key, newPresences });
+        this.lastPingTime = Date.now();
